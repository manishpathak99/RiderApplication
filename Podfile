  pod 'ObjectMapper'
  pod 'NVActivityIndicatorView', '~> 4.4.0'
  pod 'Kingfisher', '~> 5.0'
  pod 'RealmSwift'
  pod 'SnapKit'
#  pod 'SwiftLint'
end

target 'RiderAppTests' do
  use_frameworks!
  pod 'Quick'
  pod 'Nimble'
  pod 'ObjectMapper'
  pod 'RealmSwift'
end

